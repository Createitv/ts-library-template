{
  "name": "--libraryname--",
  "version": "0.0.1",
  "description": "",
  "keywords": [],
  "main": "dist/--libraryname--.umd.js",
  "module": "dist/--libraryname--.es5.js",
  "typings": "dist/types/--libraryname--.d.ts",
  "files": [
    "dist"
  ],
  "author": "--username-- <--usermail-->",
  "repository": {
    "type": "git",
    "url": "https://github.com/Createitv/ts-library-template"
  },
  "license": "MIT",
  "engines": {
    "node": ">=14.0.0"
  },
  "scripts": {
    "lint": "eslint ./src ./test  --ext .js --ext .ts --fix --color",
    "dev": "esno watch src/ts-library-template.ts",
    "prelint": "npx prettier --write src/**/*.ts test/**/*.ts",
    "prebuild": "rimraf dist",
    "build": "tsc --module commonjs && rollup -c rollup.config.ts && typedoc --out docs --entryPoints src/--libraryname--.ts --theme default",
    "start": "rollup -c rollup.config.ts -w",
    "test": "jest --coverage",
    "test:watch": "jest --coverage --watch",
    "test:prod": "npm run lint && npm run test -- --no-cache",
    "deploy-docs": "ts-node tools/gh-pages-publish",
    "prereport-coverage": "npm run test",
    "report-coverage": "cat ./coverage/lcov.info | coveralls",
    "precommit": "lint-staged",
    "commit": "git add -A && git-cz",
    "semantic-release-prepare": "ts-node tools/semantic-release-prepare",
    "semantic-release": "semantic-release",
    "travis-deploy-once": "travis-deploy-once",
    "postinstall": "ts-node tools/init"
  },
  "lint-staged": {
    "{src,test}/**/*.ts": [
      "prettier --write"
    ]
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testEnvironment": "node",
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/test/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 95,
        "lines": 95,
        "statements": 95
      }
    },
    "collectCoverageFrom": [
      "src/*.{js,ts}"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "^17.2.0",
    "@commitlint/config-conventional": "^17.2.0",
    "@rollup/plugin-commonjs": "^23.0.2",
    "@rollup/plugin-json": "^5.0.1",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-terser": "^0.2.0",
    "@types/jest": "^29.2.2",
    "@types/node": "^18.11.9",
    "@typescript-eslint/eslint-plugin": "^5.42.1",
    "@typescript-eslint/parser": "^5.42.1",
    "colors": "^1.3.2",
    "commitizen": "^4.2.4",
    "coveralls": "^3.0.2",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.18.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "husky": "^8.0.2",
    "jest": "^29.3.0",
    "jest-config": "^29.3.0",
    "lint-staged": "^13.0.3",
    "lodash.camelcase": "^4.3.0",
    "prettier": "^2.7.1",
    "prompt": "^1.0.0",
    "replace-in-file": "^6.3.2",
    "rimraf": "^3.0.2",
    "rollup": "^2.70.1",
    "rollup-plugin-sourcemaps": "^0.6.3",
    "rollup-plugin-typescript2": "^0.34.1",
    "semantic-release": "^19.0.2",
    "shelljs": "^0.8.3",
    "travis-deploy-once": "^5.0.9",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.7.0",
    "typedoc": "^0.23.20",
    "typescript": "^4.8.4"
  }
}
